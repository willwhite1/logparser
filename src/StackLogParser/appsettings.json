{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "OptimizationOptions": {
    "MaxThreadCount": 1000
  },
  "LogEntryOptions": {
    "ColumnSplitter": ",",
    "ExpectedColumnCount":  6
  }
}
